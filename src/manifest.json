{
    "name": "collect youtube",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "icons": {
        "64": "icon-64.png",
        "512": "icon-512.png"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "64": "icon-64.png"
        },
        "default_title": "collect youtube links",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}